{
  "name": "datagaze-sandbox",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "build:swc": "yarn swc --out-dir dist -w src",
    "start:swc": "nodemon dist/main",
    "start": "yarn build && node dist/main",
    "start:dev": "concurrently \"yarn build:swc\" \"yarn start:swc\"",
    "start:prod": "node dist/main",
    "migrate:make": "ts-node --transpile-only ./node_modules/.bin/knex --knexfile src/database/knexfile.ts migrate:make",
    "migrate:latest": "ts-node --transpile-only ./node_modules/.bin/knex --knexfile src/database/knexfile.ts migrate:latest",
    "migrate:rollback": "ts-node --transpile-only ./node_modules/.bin/knex --knexfile src/database/knexfile.ts migrate:rollback",
    "migrate:rollback:all": "ts-node --transpile-only ./node_modules/.bin/knex --knexfile src/database/knexfile.ts migrate:rollback --all",
    "seed:make": "ts-node --transpile-only ./node_modules/.bin/knex --knexfile src/database/knexfile.ts seed:make",
    "seed:run": "ts-node --transpile-only ./node_modules/.bin/knex --knexfile src/database/knexfile.ts seed:run",
    "db:reset": "yarn migrate:rollback:all && yarn migrate:latest && yarn seed:run"
  },
  "dependencies": {
    "@nestjs/cli": "^11.0.6",
    "@nestjs/common": "^11.0.19",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.19",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/platform-socket.io": "^11.0.13",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/websockets": "^11.0.10",
    "@types/knex": "^0.16.1",
    "@types/multer": "^1.4.12",
    "adm-zip": "^0.5.16",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.7",
    "form-data": "^4.0.2",
    "knex": "^3.1.0",
    "nestjs-knex": "^2.0.0",
    "node-scp": "^0.0.23",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.1.0",
    "socket.io": "^4.8.1",
    "ssh2": "^1.16.0",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.18",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.1",
    "@types/node": "^20.3.1",
    "@types/ssh2": "^1.15.5",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "5.7",
    "typescript-eslint": "^8.29.0"
  }
}
